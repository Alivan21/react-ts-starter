{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^react-dom/*",
    "<THIRD_PARTY_MODULES>",
    "^@tanstack/*",
    "^@/*",
    "^[.]",
    "^/",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
